{
  "name": "ngx-monaco",
  "version": "1.0.0",
  "description": "Monaco Code Editor For Angular",
  "keywords": [
    "angular",
    "monaco",
    "monaco-editor",
    "code",
    "editor",
    "reusable"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "bash scripts/build-release",
    "test": "echo \"Error: no test... minimal project\" && exit 1",
    "lint": "echo \"Error: no lint... minimal project\" && exit 1",
    "e2e": "echo \"Error: no e2e... minimal project\" && exit 1",
    "bump-dev": "gulp bump-version",
    "bump-patch": "gulp bump-version --ver patch",
    "bump-minor": "gulp bump-version --ver minor",
    "bump-major": "gulp bump-version --ver major",
    "aot": "./node_modules/.bin/ngc -p deploy/platform/tsconfig-aot.json && tsc -p src/platform/tsconfig-aot.json",
    "minify": "uglifyjs deploy/platform/editor/editor.umd.js --screw-ie8 --compress --mangle --comments --output deploy/platform/editor/editor.umd.min.js",
    "npm-publish": "npm run build && bash scripts/npm-publish"
  },
  "engines": {
    "node": ">=6.9",
    "npm": ">3"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/pycraft114/ngx-monaco.git"
  },
  "bugs": {
    "url": "https://github.com/pycraft114/ngx-monaco.git/issues"
  },
  "author": "pycraft114",
  "contributors": [
    "pycraft114 <pycraft114@gmail.com>"
  ],
  "dependencies": {
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "core-js": "^2.5.3",
    "monaco-editor": "^0.10.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-help": "^1.6.1",
    "gulp-if": "^2.0.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.2",
    "gulp-util": "^3.0.8",
    "merge2": "^1.2.0",
    "minimist": "^1.2.0",
    "require-dir": "^0.3.2",
    "rollup": "^0.53.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semver": "^5.4.1",
    "typescript": "~2.5.3",
    "uglify-js": "^3.3.3"
  }
}
